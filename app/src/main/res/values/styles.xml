<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundedImageView">
        <item name="shapeAppearance">@style/RoundedImageView.ShapeAppearance</item>
        <item name="shapeAppearanceOverlay">@style/RoundedImageView.ShapeAppearance</item>
    </style>

    <style name="RoundedImageView.ShapeAppearance">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

    <style name="RoundedImageView2">
        <item name="shapeAppearance">@style/RoundedImageView2.ShapeAppearance</item>
        <item name="shapeAppearanceOverlay">@style/RoundedImageView2.ShapeAppearance</item>
    </style>
    <style name="RoundedImageView2.ShapeAppearance">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">25dp</item>
    </style>
    <style name="RoundedImageView4">
        <item name="shapeAppearance">@style/RoundedImageView2.ShapeAppearance</item>
        <item name="shapeAppearanceOverlay">@style/RoundedImageView2.ShapeAppearance</item>
    </style>
    <style name="RoundedImageView4.ShapeAppearance">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="RoundedImageView3">
        <item name="shapeAppearance">@style/RoundedImageView2.ShapeAppearance</item>
